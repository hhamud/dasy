(defenum Roles
    ADMIN
    USER)

(defvar roles (public (hash-map :address Roles)))

(defn __init__ [] :external
  (set-at self/roles msg/sender Roles/ADMIN))

(defn getPrice [] :uint256 [:external :view]
      (if (== (get-at self/roles msg/sender) Roles/ADMIN)
          (return 10)
          (return 20)))
