(defn useIf [:uint256 x] :uint256 :external
  (if (<= x 10)
      1
      (if (<= x 20)
          2
          3)))

(defn useCond [:uint256 x] :uint256 :external
  (cond
    (<= x 10) 1
    (<= x 20) 2
    :else 3))

(defn useCondp [:uint256 x] :uint256 :external
  (condp <= x
         10 1
         20 2
         :else 3))

(defn absoluteValue [:uint256 x y] :uint256 [:external :pure]
  (if (>= x y)
       (- x y)
       (- y x)))
